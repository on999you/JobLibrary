<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_relevant_salary_data"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="?attr/actionBarSize"
    tools:context="com.example.dennislam.myapplication.activity.RelevantDataActivity">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:gravity="center"
        android:theme="@style/titleBar"
        android:text="@string/relevantSalaryD"
        android:textSize="20sp"
        android:textColor="#FFFFFF"/>
    <com.sch.rfview.AnimRFRecyclerView
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/refresh_layout2"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </com.sch.rfview.AnimRFRecyclerView>

</LinearLayout>
